{
  "name": "@effect-migrate/core",
  "version": "0.4.0",
  "description": "Core engine for Effect migration tooling",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aridyckovsky/effect-migrate.git",
    "directory": "packages/core"
  },
  "type": "module",
  "main": "./build/esm/index.js",
  "types": "./build/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/esm/index.d.ts",
      "import": "./build/esm/index.js"
    },
    "./schema": {
      "types": "./build/esm/schema/index.d.ts",
      "import": "./build/esm/schema/index.js"
    },
    "./amp": {
      "types": "./build/esm/amp/index.d.ts",
      "import": "./build/esm/amp/index.js"
    }
  },
  "sideEffects": [],
  "files": [
    "build",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.14.0"
  },
  "scripts": {
    "clean": "rimraf build dist",
    "build:ts": "tsc -b tsconfig.build.json",
    "build:esm": "babel build/esm --out-dir build/esm --plugins babel-plugin-annotate-pure-calls --source-maps",
    "build": "pnpm run clean && pnpm run build:ts && pnpm run build:esm",
    "pack": "build-utils pack-v4",
    "test": "vitest",
    "test:ci": "vitest run",
    "typecheck": "tsc -b",
    "typecheck:src": "tsc -b tsconfig.src.json",
    "typecheck:test": "tsc -b tsconfig.test.json",
    "lint": "eslint . --max-warnings 0 && prettier --check \"**/*.{json,md,yml,yaml}\"",
    "lint:fix": "eslint . --fix && prettier --write \"**/*.{json,md,yml,yaml}\""
  },
  "dependencies": {
    "@effect/platform": "0.93.0",
    "@effect/platform-node": "0.100.0",
    "effect": "3.19.2",
    "tsx": "^4.20.6"
  },
  "devDependencies": {
    "@effect/vitest": "^0.27.0",
    "@types/node": "^24.10.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "effect": "^3.0.0"
  }
}
